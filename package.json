{
  "name": "obsidian-latex-environments",
  "version": "0.1.1",
  "description": "Quickly insert and change latex environments within math blocks in Obsidian.",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "commit": "git-cz",
    "release": "standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "tag-prefix": "",
    "bumpFiles": [
      "package.json",
      "manifest.json",
      {
        "filename": "versions.json",
        "updater": ".standard-version/standard-version-versions-updater.js"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:raineszm/obsidian-latex-environments.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/codemirror": "^0.0.98",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codemirror": "^5.58.2",
    "commitizen": "^4.2.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/tarball/master",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-copy": "^3.3.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
